package java6_8;

public class java6_8 {

	public static void main(String[] args) {
		MyRectang1 mr = new MyRectang1();
		mr.depth = 10;
		mr.width = 20;

		System.out.println(mr.getArea());

		MyRectang1 mr1 = new MyRectang1(10,20);
		System.out.println(mr1.getArea());
		MyRectang1 mr2 = new MyRectang1();

		System.out.println(mr1.equals(mr2));

		System.out.println(mr.equals(mr1));
		System.out.println(mr.hashCode());
		System.out.println(mr1.hashCode());
		System.out.println(mr2.hashCode());

	}

}
--------------------------------------------------
package java6_8;

public class MyRectang {
	double width;
	double depth;
	String code;

	public MyRectang(double depth, double width) {
		this.width = width;
		this.depth = depth;

	}

	public MyRectang() {

	}

	void setwidth(double width) {
		this.width = width;
	}

	void setdepth(double depth) {
		this.depth = depth;
	}

	double getArea() {
		double area = depth * width;
		return area;
	}

	public boolean equals(Object obj) {
		return true;
	}

	public int hashCode() {

		return 1;

	}

}

class MyRectang1 extends MyRectang {

	public int hashCode() {

		return new Double(width).hashCode() ^ new Double(depth).hashCode();

	}

	public MyRectang1(double depth, double width) {
		super(depth, width);

	}

	public MyRectang1() {

	}

	public boolean equals(Object obj) {

		if (obj instanceof MyRectang1 && obj != null) {

			MyRectang mr = (MyRectang1) obj;

			if (this.depth == mr.depth && this.width == mr.width) {
				return true;
			} else {
				return false;

			}
		} else {
			return false;
		}
	}

	public boolean equals1(Object obj) {
		if (this.equals(obj)) {
			return true;
		} else {
			return false;
		}
	}
}
